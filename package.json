{
  "name": "ts-starter",
  "version": "0.0.0",
  "description": "A bare minimum starter repository for writing and testing browser modules using typescript.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "prettier --print-width 120 --single-quote --trailing-comma es5 --write \"src/**/*.ts\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "prepublish": "npm run lint && npm run build"
  },
  "author": "Anonymous",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.7.1",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.2",
    "webpack": "^4.29.6"
  }
}
